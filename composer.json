{
    "name": "bfg/puller",
    "description": "A description for puller.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Xsaven",
            "email": "xsaven@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Bfg\\Puller\\": "./src"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Bfg\\Puller\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bfg\\Puller\\PullerServiceProvider"
            ],
            "aliases": {
                "Puller": "Bfg\\Puller\\Facades\\PullerFacade"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    }
}
